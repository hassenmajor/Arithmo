<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.major.arithmo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FullActivity"
            android:exported="true"
            android:label="@string/layout_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".SystemActivity"
            android:label="@string/title_activity_system" />
        <activity
            android:name=".CurveActivity"
            android:label="@string/title_activity_curve" />
        <activity
            android:name=".MatrixActivity"
            android:label="@string/title_activity_matrix" />
        <activity
            android:name=".UnitActivity"
            android:label="@string/title_activity_unit" />
        <activity
            android:name=".HelpActivity"
            android:label="@string/title_activity_help" />
    </application>

</manifest>